            border-radius: 3px;
        }

        .crosscheck input[type="checkbox"]:checked {
            background: #8a8a8a;
            border: 0;
        }

        .crosscheck input[type="checkbox"]:checked:after {
            content: "X";
            color: #fff;
            position: absolute;
            left: 50%;
            top: 50%;
            -webkit-transform: translate(-50%, -50%);
            -moz-transform: translate(-50%, -50%);
            -ms-transform: translate(-50%, -50%);
